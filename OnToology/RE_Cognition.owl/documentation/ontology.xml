        <rdfs:label>cluster name</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/recognition/Cim#granularity -->


    <owl:DatatypeProperty rdf:about="http://purl.org/recognition/Cim#granularity">
